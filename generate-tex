pandoc --latex-engine=xelatex -f markdown -t latex latex/UALib/Prelude/Extensionality.md -o latex/UALib/Prelude/Extensionality.tex
pandoc --latex-engine=xelatex -f markdown -t latex latex/UALib/Prelude/Inverses.md -o latex/UALib/Prelude/Inverses.tex
pandoc --latex-engine=xelatex -f markdown -t latex latex/UALib/Prelude/Preliminaries.md -o latex/UALib/Prelude/Preliminaries.tex
pandoc --latex-engine=xelatex -f markdown -t latex latex/UALib/Relations/Binary.md -o latex/UALib/Relations/Binary.tex
pandoc --latex-engine=xelatex -f markdown -t latex latex/UALib/Relations/Congruences.md -o latex/UALib/Relations/Congruences.tex
pandoc --latex-engine=xelatex -f markdown -t latex latex/UALib/Relations/Equivalences.md -o latex/UALib/Relations/Equivalences.tex
pandoc --latex-engine=xelatex -f markdown -t latex latex/UALib/Relations/Quotients.md -o latex/UALib/Relations/Quotients.tex
pandoc --latex-engine=xelatex -f markdown -t latex latex/UALib/Relations/Unary.md -o latex/UALib/Relations/Unary.tex
pandoc --latex-engine=xelatex -f markdown -t latex latex/UALib/Subalgebras/Generation.md -o latex/UALib/Subalgebras/Generation.tex
pandoc --latex-engine=xelatex -f markdown -t latex latex/UALib/Subalgebras/Homomorphisms.md -o latex/UALib/Subalgebras/Homomorphisms.tex
pandoc --latex-engine=xelatex -f markdown -t latex latex/UALib/Subalgebras/Properties.md -o latex/UALib/Subalgebras/Properties.tex
pandoc --latex-engine=xelatex -f markdown -t latex latex/UALib/Subalgebras/Subalgebras.md -o latex/UALib/Subalgebras/Subalgebras.tex
pandoc --latex-engine=xelatex -f markdown -t latex latex/UALib/Subalgebras/Subuniverses.md -o latex/UALib/Subalgebras/Subuniverses.tex
