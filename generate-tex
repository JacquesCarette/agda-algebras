#!/bin/bash

echo "Checking UALib.Algebras.Algebras"
agda --latex UALib/Algebras/Algebras.lagda
echo "Checking UALib.Algebras.Lifts.lagda"
agda --latex UALib/Algebras/Lifts.lagda
echo "Checking UALib.Algebras.Products.lagda"
agda --latex UALib/Algebras/Products.lagda
echo "Checking UALib.Algebras.Signatures.lagda"
agda --latex UALib/Algebras/Signatures.lagda
echo "Checking UALib.Birkhoff.FreeAlgebra.lagda"
agda --latex UALib/Birkhoff/FreeAlgebra.lagda
echo "Checking UALib.Birkhoff.HSPTheorem.lagda"
agda --latex UALib/Birkhoff/HSPTheorem.lagda
echo "Checking UALib.Homomorphisms.Basic.lagda"
agda --latex UALib/Homomorphisms/Basic.lagda
echo "Checking UALib.Homomorphisms.HomomorphicImages.lagda"
agda --latex UALib/Homomorphisms/HomomorphicImages.lagda
echo "Checking UALib.Homomorphisms.Isomorphisms.lagda"
agda --latex UALib/Homomorphisms/Isomorphisms.lagda
echo "Checking UALib.Homomorphisms.Kernels.lagda"
agda --latex UALib/Homomorphisms/Kernels.lagda
echo "Checking UALib.Homomorphisms.Noether.lagda"
agda --latex UALib/Homomorphisms/Noether.lagda
echo "Checking UALib.Homomorphisms.Products.lagda"
agda --latex UALib/Homomorphisms/Products.lagda
echo "Checking UALib.Prelude.Equality.lagda"
agda --latex UALib/Prelude/Equality.lagda
echo "Checking UALib.Prelude.Extensionality.lagda"
agda --latex UALib/Prelude/Extensionality.lagda
echo "Checking UALib.Prelude.Inverses.lagda"
agda --latex UALib/Prelude/Inverses.lagda
echo "Checking UALib.Prelude.Preliminaries.lagda"
agda --latex UALib/Prelude/Preliminaries.lagda
echo "Checking UALib.Relations.Binary.lagda"
agda --latex UALib/Relations/Binary.lagda
echo "Checking UALib.Relations.Congruences.lagda"
agda --latex UALib/Relations/Congruences.lagda
echo "Checking UALib.Relations.Equivalences.lagda"
agda --latex UALib/Relations/Equivalences.lagda
echo "Checking UALib.Relations.Quotients.lagda"
agda --latex UALib/Relations/Quotients.lagda
echo "Checking UALib.Relations.Unary.lagda"
agda --latex UALib/Relations/Unary.lagda
echo "Checking UALib.Subalgebras.Generation.lagda"
agda --latex UALib/Subalgebras/Generation.lagda
echo "Checking UALib.Subalgebras.Homomorphisms.lagda"
agda --latex UALib/Subalgebras/Homomorphisms.lagda
echo "Checking UALib.Subalgebras.Properties.lagda"
agda --latex UALib/Subalgebras/Properties.lagda
echo "Checking UALib.Subalgebras.Subalgebras.lagda"
agda --latex UALib/Subalgebras/Subalgebras.lagda
echo "Checking UALib.Subalgebras.Subuniverses.lagda"
agda --latex UALib/Subalgebras/Subuniverses.lagda
echo "Checking UALib.Subalgebras.WWMD.lagda"
agda --latex UALib/Subalgebras/WWMD.lagda
echo "Checking UALib.Terms.Basic.lagda"
agda --latex UALib/Terms/Basic.lagda
echo "Checking UALib.Terms.Compatibility.lagda"
agda --latex UALib/Terms/Compatibility.lagda
echo "Checking UALib.Terms.Free.lagda"
agda --latex UALib/Terms/Free.lagda
echo "Checking UALib.Terms.Operations.lagda"
agda --latex UALib/Terms/Operations.lagda
echo "Checking UALib.Varieties.ClassProducts.lagda"
agda --latex UALib/Varieties/ClassProducts.lagda
echo "Checking UALib.Varieties.EquationalLogic.lagda"
agda --latex UALib/Varieties/EquationalLogic.lagda
echo "Checking UALib.Varieties.ModelTheory.lagda"
agda --latex UALib/Varieties/ModelTheory.lagda
echo "Checking UALib.Varieties.Preservation.lagda"
agda --latex UALib/Varieties/Preservation.lagda
echo "Checking UALib.Varieties.Varieties.lagda"
agda --latex UALib/Varieties/Varieties.lagda

mv latex/UALib/Algebras/Algebras.{tex,md}
mv latex/UALib/Algebras/Lifts.{tex,md}
mv latex/UALib/Algebras/Products.{tex,md}
mv latex/UALib/Algebras/Signatures.{tex,md}
mv latex/UALib/Birkhoff/FreeAlgebra.{tex,md}
mv latex/UALib/Birkhoff/HSPTheorem.{tex,md}
mv latex/UALib/Homomorphisms/Basic.{tex,md}
mv latex/UALib/Homomorphisms/HomomorphicImages.{tex,md}
mv latex/UALib/Homomorphisms/Isomorphisms.{tex,md}
mv latex/UALib/Homomorphisms/Kernels.{tex,md}
mv latex/UALib/Homomorphisms/Noether.{tex,md}
mv latex/UALib/Homomorphisms/Products.{tex,md}
mv latex/UALib/Prelude/Equality.{tex,md}
mv latex/UALib/Prelude/Extensionality.{tex,md}
mv latex/UALib/Prelude/Inverses.{tex,md}
mv latex/UALib/Prelude/Preliminaries.{tex,md}
mv latex/UALib/Relations/Binary.{tex,md}
mv latex/UALib/Relations/Congruences.{tex,md}
mv latex/UALib/Relations/Equivalences.{tex,md}
mv latex/UALib/Relations/Quotients.{tex,md}
mv latex/UALib/Relations/Unary.{tex,md}
mv latex/UALib/Subalgebras/Generation.{tex,md}
mv latex/UALib/Subalgebras/Homomorphisms.{tex,md}
mv latex/UALib/Subalgebras/Properties.{tex,md}
mv latex/UALib/Subalgebras/Subalgebras.{tex,md}
mv latex/UALib/Subalgebras/Subuniverses.{tex,md}
mv latex/UALib/Subalgebras/WWMD.{tex,md}
mv latex/UALib/Terms/Basic.{tex,md}
mv latex/UALib/Terms/Compatibility.{tex,md}
mv latex/UALib/Terms/Free.{tex,md}
mv latex/UALib/Terms/Operations.{tex,md}
mv latex/UALib/Varieties/ClassProducts.{tex,md}
mv latex/UALib/Varieties/EquationalLogic.{tex,md}
mv latex/UALib/Varieties/ModelTheory.{tex,md}
mv latex/UALib/Varieties/Preservation.{tex,md}
mv latex/UALib/Varieties/Varieties.{tex,md}

pandoc --latex-engine=xelatex -f markdown -t latex latex/UALib/Algebras/Algebras.md -o latex/UALib/Algebras/Algebras.tex
pandoc --latex-engine=xelatex -f markdown -t latex latex/UALib/Algebras/Lifts.md -o latex/UALib/Algebras/Lifts.tex
pandoc --latex-engine=xelatex -f markdown -t latex latex/UALib/Algebras/Products.md -o latex/UALib/Algebras/Products.tex
pandoc --latex-engine=xelatex -f markdown -t latex latex/UALib/Algebras/Signatures.md -o latex/UALib/Algebras/Signatures.tex
pandoc --latex-engine=xelatex -f markdown -t latex latex/UALib/Birkhoff/FreeAlgebra.md -o latex/UALib/Birkhoff/FreeAlgebra.tex
pandoc --latex-engine=xelatex -f markdown -t latex latex/UALib/Birkhoff/HSPTheorem.md -o latex/UALib/Birkhoff/HSPTheorem.tex
pandoc --latex-engine=xelatex -f markdown -t latex latex/UALib/Homomorphisms/Basic.md -o latex/UALib/Homomorphisms/Basic.tex
pandoc --latex-engine=xelatex -f markdown -t latex latex/UALib/Homomorphisms/HomomorphicImages.md -o latex/UALib/Homomorphisms/HomomorphicImages.tex
pandoc --latex-engine=xelatex -f markdown -t latex latex/UALib/Homomorphisms/Isomorphisms.md -o latex/UALib/Homomorphisms/Isomorphisms.tex
pandoc --latex-engine=xelatex -f markdown -t latex latex/UALib/Homomorphisms/Kernels.md -o latex/UALib/Homomorphisms/Kernels.tex
pandoc --latex-engine=xelatex -f markdown -t latex latex/UALib/Homomorphisms/Noether.md -o latex/UALib/Homomorphisms/Noether.tex
pandoc --latex-engine=xelatex -f markdown -t latex latex/UALib/Homomorphisms/Products.md -o latex/UALib/Homomorphisms/Products.tex
