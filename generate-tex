pandoc --latex-engine=xelatex -f markdown -t latex latex/UALib/Terms/Basic.md -o latex/UALib/Terms/Basic.tex
pandoc --latex-engine=xelatex -f markdown -t latex latex/UALib/Terms/Compatibility.md -o latex/UALib/Terms/Compatibility.tex
pandoc --latex-engine=xelatex -f markdown -t latex latex/UALib/Terms/Free.md -o latex/UALib/Terms/Free.tex
pandoc --latex-engine=xelatex -f markdown -t latex latex/UALib/Terms/Operations.md -o latex/UALib/Terms/Operations.tex
pandoc --latex-engine=xelatex -f markdown -t latex latex/UALib/Varieties/ClassProducts.md -o latex/UALib/Varieties/ClassProducts.tex
